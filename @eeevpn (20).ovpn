client
dev tun
proto tcp

# Multi-endpoint (Load Balancing)

remote 45.154.153.24 443

remote-random

remote-cert-tls server
auth-nocache
nobind
persist-key
persist-tun
reneg-sec 0
cipher AES-256-CBC
auth SHA512
verb 3
redirect-gateway def1

route 0.0.0.0 0.0.0.0 vpn_gateway

auth-user-pass

# ================================
#      Anti-Reset / Anti-Drop
# ================================
connect-retry-max 9999
connect-retry 1 2
connect-timeout 5
inactive 0
ping 5
ping-restart 20
setenv SAFE_RESTART 1
mute-replay-warnings
explicit-exit-notify 1

<auth-user-pass>
WsVGthaAeu
WyagEqzSPj
</auth-user-pass>


# ================================
#       Adaptive MTU (AI Mode)
# ================================
mssfix 0
tun-mtu 1500

sndbuf 524288
rcvbuf 524288
push-peer-info
setenv MTU_ADAPTIVE 1

